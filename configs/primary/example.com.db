; SOA record
; your@example.com turn in your.email.com
@                   IN  SOA   example.com. your.email.com. (
                        2015082541  ; serial
                        7200        ; refresh
                        3600        ; retry
                        1209600     ; expire
                        3600        ; minimun
                    )
                     IN A 192.168.1.1

; multiple ips for a domain
host.example.com.     IN  A       192.168.1.1
                      IN  A       192.168.1.2
                      IN  A       192.168.1.3
                      IN  AAAA    2804:14d:5cde:440f:bd45:e882:c3eb:413b
                      IN  AAAA    2804:14d:5cde:440f:bd45:e882:c3eb:413c

alias.example.com.    IN  CNAME   host.example.com.
alias2.example.com.   IN  CNAME   alias.example.com.

out.example.com.      IN CNAME   google.com.

; www not will works (why ?)
www.example.com.      IN  CNAME   alias.example.com.